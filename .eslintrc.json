{
    "rules": {
        "indent": ["error", 4, {
        "SwitchCase": 1
        }],
        "max-len": ["error", 300],
        "space-before-function-paren": [
            2, {
                "anonymous": "always",
                "named": "never"
            }
        ],
        "semi": 1,
        "no-plusplus": 0,
        "func-names": ["error", "never"],
        "arrow-parens": 0,
        "arrow-body-style": 0,
        "no-use-before-define": [
            2,
            "nofunc"
        ],
        "no-console": 0,
        "no-await-in-loop": 0,
        "no-param-reassign": 0,
        "global-require": 0,
        "no-else-return": 0,
        "no-prototype-builtins": 0,
        "comma-dangle": [
            0
        ],
        "import/no-extraneous-dependencies": [
            "error", {
                "devDependencies": true
            }
        ],
        "no-underscore-dangle": [
            0
        ],
        "no-restricted-syntax": ["error", "ForInStatement", "LabeledStatement", "WithStatement"]
    },
    "env": {
        "node": true,
        "mocha": true
    },
    "globals": {
      "angular": true,
      "Chart": true,
      "moment": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "extends": [
        "eslint:recommended",
        "airbnb-base"
    ]
}